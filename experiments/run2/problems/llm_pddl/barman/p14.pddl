

(define (problem barman-problem)
  (:domain barman)
  (:objects
    shaker - container
    shot1 shot2 shot3 shot4 shot5 shot6 shot7 - shotglass
    ingredient1 ingredient2 ingredient3 - ingredient
    )
  (:init
    (empty shaker)
    (empty shot1)
    (empty shot2)
    (empty shot3)
    (empty shot4)
    (empty shot5)
    (empty shot6)
    (empty shot7)
    (clean shaker)
    (clean shot1)
    (clean shot2)
    (clean shot3)
    (clean shot4)
    (clean shot5)
    (clean shot6)
    (clean shot7)
    (on-table shaker)
    (on-table shot1)
    (on-table shot2)
    (on-table shot3)
    (on-table shot4)
    (on-table shot5)
    (on-table shot6)
    (on-table shot7)
    (level shaker 0)
    (contains shot1 ingredient3)
    (contains shot1 ingredient2)
    (contains shot2 ingredient2)
    (contains shot2 ingredient1)
    (contains shot3 ingredient3)
    (contains shot3 ingredient1)
    (contains shot4 ingredient2)
    (contains shot4 ingredient1)
    (contains shot5 ingredient2)
    (contains shot5 ingredient1)
    (contains shot6 ingredient1)
    (contains shot6 ingredient2)
    (contains shot7 ingredient2)
  )
  (:goal (and
    (contains shaker cocktail3)
    (contains shaker cocktail1)
    (contains shaker cocktail4)
    (contains shaker cocktail6)
    (contains shaker cocktail2)
    (contains shaker cocktail5)
  ))
)