

(define (problem barman-problem)
  (:domain barman)
  (:objects
    shaker - container
    shot1 shot2 shot3 shot4 - shotglass
    ingredient1 ingredient2 ingredient3 - ingredient
    )
  (:init
    (empty shaker)
    (empty shot1)
    (empty shot2)
    (empty shot3)
    (empty shot4)
    (clean shaker)
    (clean shot1)
    (clean shot2)
    (clean shot3)
    (clean shot4)
    (on-table shaker)
    (on-table shot1)
    (on-table shot2)
    (on-table shot3)
    (on-table shot4)
    (hand-empty left)
    (hand-empty right)
    (level shaker 0)
    (level shot1 0)
    (level shot2 0)
    (level shot3 0)
    (level shot4 0)
    (ingredient-in-dispenser ingredient1)
    (ingredient-in-dispenser ingredient2)
    (ingredient-in-dispenser ingredient3)
    )
  (:goal (and
    (filled shot1)
    (filled shot2)
    (filled shot3)
    (filled shot4)
    (shaked shaker)
    (contains-ingredient shot1 ingredient2)
    (contains-ingredient shot1 ingredient1)
    (contains-ingredient shot2 ingredient3)
    (contains-ingredient shot2 ingredient2)
    (contains-ingredient shot3 ingredient1)
    (contains-ingredient shot3 ingredient3)
    (contains-ingredient shot4 ingredient2)
    (contains-ingredient shot4 ingredient1)
    )
  )
)